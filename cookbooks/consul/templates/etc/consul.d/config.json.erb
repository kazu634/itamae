{
  "datacenter": "aws",
  <% if @manager then%>
  "bootstrap_expect": 3,
  "addresses": {
    "http": "0.0.0.0"
  },
  "ui": true,
  <% end %>
  "bind_addr": "<%= @ipaddr %>",
  "disable_remote_exec": false,
  "disable_update_check": true,
  "leave_on_terminate": true,
  "retry_interval": "30s",
  "data_dir": "/var/opt/consul",
  "log_level": "INFO",
  "enable_syslog": false,
  "enable_script_checks": true,
  "rejoin_after_leave": true,
  "retry_join": <%= @manager_hosts %>,
  "encrypt": "LPKrNBQZnJIc8tJpViI4ug==",
  "server": <%= @manager %>
}
